{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "skipLibCheck": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": false,
    "strict": false,
    "noImplicitAny": false,
    "allowImportingTsExtensions": true,
    "module": "ESNext",
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
